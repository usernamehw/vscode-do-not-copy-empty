{
    "name": "do-not-copy-empty",
    "displayName": "DEPRECATED: do-not-copy-empty",
    "description": "Prevent copying of empty lines",
    "version": "1.0.1",
    "publisher": "usernamehw",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/usernamehw/vscode-do-not-copy-empty"
    },
    "bugs": {
        "url": "https://github.com/usernamehw/vscode-do-not-copy-empty/issues"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.doNotCopyEmpty"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.doNotCopyEmpty",
                "title": "Do not copy empty"
            }
        ],
        "keybindings": [
            {
                "command": "extension.doNotCopyEmpty",
                "key": "ctrl+c",
                "mac": "cmd+c",
                "when": "!editorHasSelection && editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
