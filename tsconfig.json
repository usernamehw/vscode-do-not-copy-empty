{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es6"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,   /* enable all strict type-checking options */
		"noUnusedLocals": true, /* Report errors on unused locals. */
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
